[editor_plugins]

enabled=PackedStringArray("res://addons/level_block/plugin.cfg")

[input]

forward={
"deadzone": 0.5,
"events": []
}
backward={
"deadzone": 0.5,
"events": []
}
left={
"deadzone": 0.5,
"events": []
}
right={
"deadzone": 0.5,
"events": []
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
